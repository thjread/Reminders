{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": [false],
        "interface-name": [false],
        "object-literal-sort-keys": [false],
        "object-literal-key-quotes": [true, "as-needed"],
        "whitespace": [true,
                       "check-branch",
                       "check-decl",
                       "check-module",
                       "check-separator",
                       "check-rest-spread",
                       "check-type",
                       "check-typecast",
                       "check-type-operator",
                       "check-preblock",
                       "check-postbrace"]
    },
    "rulesDirectory": []
}
